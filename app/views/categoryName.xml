			<ImageView id="backButton" class="backButton"></ImageView>
			<Label id="categoryName" class="PageHeading"></Label>
		</View>
		<View id="matrix">
			<View id="matrixType">
				<Label id="matrixOf2" class="matrixOfAll" >2X2</Label>
				<Label id="matrixOf3" class="matrixOfAll" >3X3</Label>
